<div class="navi">
<%=
  footer_navis = if defined?(::RWiki::FooterNavis)
                   ::RWiki::FooterNavis
                 else
                   []
                 end
  pg.book.navi.collect { |title, page|
    unless footer_navis.include?(title)
      page.navi_view(title, pg, @env, &@block)
    end
  }.compact.join("\n")
%>
<% if defined?(::RWiki::FooterNavis) %>
<span class="navi">[<a href="#footer-navi">more...</a>]</span>
<% end %>
</div>
